            </widget>

            <widget name="Leave" factory="pawsButton" id="306" style="Standard Button">
                <frame x="445" y="3" width="70" height="20" />
                <label text="Leave" />
            </widget>

            <widget name="MemberList" factory="pawsListBox" xmlbinding="memberlist" >
