            </widget>

            <widget name="" factory="pawsButton" tooltip="Privileges">
                <frame x="272" y="5" width="30" height="30" />
                <bgimage resource="GuildPrivilegesIcon" />
                <eventMouseDown>Ret=prioptdes:Show();Ret=chatoptdes:Hide();Ret=vchatoptdes:Hide();Ret=invoptdes:Hide();Ret=removoptdes:Hide();Ret=promooptdes:Hide();Ret=poioptdes:Hide();Ret=editoptdes:Hide();Ret=pubnoptdes:Hide();Ret=prinoptdes:Hide();Ret=valicoptdes:Hide();Ret=alicoptdes:Hide();Ret=gbanoptdes:Hide();Ret=desdefault:Hide()</eventMouseDown>
            </widget>

            <widget name="" factory="pawsButton" tooltip="Points">
                <frame x="302" y="5" width="30" height="30" />
