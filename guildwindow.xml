                <eventMouseDown>Ret=poioptdes:Show();Ret=chatoptdes:Hide();Ret=vchatoptdes:Hide();Ret=invoptdes:Hide();Ret=removoptdes:Hide();Ret=promooptdes:Hide();Ret=prioptdes:Hide();Ret=editoptdes:Hide();Ret=pubnoptdes:Hide();Ret=prinoptdes:Hide();Ret=valicoptdes:Hide();Ret=alicoptdes:Hide();Ret=gbanoptdes:Hide();Ret=desdefault:Hide()</eventMouseDown>
            </widget>

            <widget name="" factory="pawsButton" tooltip="Edit">
                <frame x="332" y="5" width="30" height="30" />
                <bgimage resource="GuildEditIcon" />
                <eventMouseDown>Ret=editoptdes:Show();Ret=chatoptdes:Hide();Ret=vchatoptdes:Hide();Ret=invoptdes:Hide();Ret=removoptdes:Hide();Ret=promooptdes:Hide();Ret=prioptdes:Hide();Ret=poioptdes:Hide();Ret=pubnoptdes:Hide();Ret=prinoptdes:Hide();Ret=valicoptdes:Hide();Ret=alicoptdes:Hide();Ret=gbanoptdes:Hide();Ret=desdefault:Hide()</eventMouseDown>
            </widget>

            <widget name="" factory="pawsButton" tooltip="Public Note">
                <frame x="362" y="5" width="30" height="30" />
                <bgimage resource="GuildPublicNoteIcon" />
                <eventMouseDown>Ret=pubnoptdes:Show();Ret=chatoptdes:Hide();Ret=vchatoptdes:Hide();Ret=invoptdes:Hide();Ret=removoptdes:Hide();Ret=promooptdes:Hide();Ret=prioptdes:Hide();Ret=poioptdes:Hide();Ret=editoptdes:Hide();Ret=prinoptdes:Hide();Ret=valicoptdes:Hide();Ret=alicoptdes:Hide();Ret=gbanoptdes:Hide();Ret=desdefault:Hide()</eventMouseDown>
            </widget>

