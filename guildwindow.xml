<widget_description>
    <widget name="GuildWindow" factory="pawsGuildWindow" savepositions="yes" visible="no" movable="yes" resizable="no" configurable="yes" style="New Standard GUI">
        <frame x="100" y="200" width="520" height="380" border="yes"/>
        <title resource="Scaling Title Bar" text="Guild" align="left" close_button="yes"/>

        <!--
            <childborders>
            <colour r="0" g="0" b="0" />
            <colour r="0" g="0" b="255" />
            <colour r="32" g="32" b="32" />
            <colour r="148" g="138" b="255" />
            <colour r="64" g="64" b="64" />
            </childborders>
        -->



        <widget name="Settings_Tab" factory="pawsButton" id="203" toggle="yes" shadows="yes" style="Standard Tab" >
            <frame x="0" y="5" width="80" height="25" />
            <attachpoints>
                <attach point="PROPORTIONAL_RIGHT" />
                <attach point="PROPORTIONAL_LEFT" />
            </attachpoints>
            <label text="Settings" />
        </widget>

        <widget name="Members_Tab" factory="pawsButton" id="201" toggle="yes" shadows="yes" style="Standard Tab" >
            <frame x="85" y="5" width="80" height="25" />
            <attachpoints>
                <attach point="PROPORTIONAL_RIGHT" />
                <attach point="PROPORTIONAL_LEFT" />
            </attachpoints>
            <label text="Members" />
        </widget>

        <widget name="Permissions_Tab" factory="pawsButton" id="200" toggle="yes" shadows="yes" style="Standard Tab" >
            <frame x="170" y="5" width="80" height="25" />
            <attachpoints>
                <attach point="PROPORTIONAL_RIGHT" />
                <attach point="PROPORTIONAL_LEFT" />
            </attachpoints>
            <label text="Permissions" />
        </widget>

        <widget name="Alliances_Tab" factory="pawsButton" id="202" toggle="yes" shadows="yes" style="Standard Tab" >
            <frame x="255" y="5" width="80" height="25" />
            <attachpoints>
                <attach point="PROPORTIONAL_RIGHT" />
                <attach point="PROPORTIONAL_LEFT" />
            </attachpoints>
            <label text="Alliance" />
        </widget>


        <!-- PERMISSIONS PANEL -->

        <widget name="Permissions_Panel" factory="pawsWidget">
            <frame x="0" y="30" width="555" height="350" border="yes"/>

            <!-- Information widgets -->

            <widget name="" factory="pawsTextBox">
                <frame x="5" y="220" width="510" height="30" />
                <text string="Select permissions icon for description:"/>
            </widget>

            <widget name="desdefault" factory="pawsMultiLineTextBox" style="Standard Font" visible="yes" border="yes">
                <frame x="5" y="245" width="510" height="40" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string=""/>
            </widget>
            <widget name="chatoptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="245" width="510" height="40" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="'View Chat' - ability to read all text in the guild tab of the 'chat window'."/>
            </widget>
            <widget name="vchatoptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="245" width="510" height="40" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="'Chat' - ability to send messages to other guild members in the guild tab of the 'chat window'."/>
            </widget>
            <widget name="invoptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="245" width="510" height="40" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="'Invite' - ability to invite other players into the guild."/>
            </widget>
            <widget name="removoptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="245" width="510" height="40" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="'Remove' - ability to remove players with lower guild levels from the guild."/>
            </widget>
            <widget name="promooptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="245" width="510" height="40" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="'Promote' - ability to promote other guild members to any level below that which the player currently has."/>
            </widget>
            <widget name="prioptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="245" width="510" height="40" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="'Privileges' - ability to adjust which permissions are active for any level below that which the player currently has."/>
            </widget>
            <widget name="poioptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="245" width="510" height="40" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="'Points' - ability to adjust the number of points of players with lower guild levels."/>
            </widget>
            <widget name="editoptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="245" width="510" height="40" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="'Edit' - ability to edit the guild settings on the 'Settings' tab of the guild window."/>
            </widget>
            <widget name="pubnoptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="245" width="510" height="40" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="'Public Note' - ability to edit the public notes of guild members with a lower guild level."/>
            </widget>
            <widget name="prinoptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="245" width="510" height="40" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="'Private Note' - ability to edit private notes of guild members with a lower guild level."/>
            </widget>
            <widget name="valicoptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="245" width="510" height="40" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="'View Alliance Chat' - ability to read all text in the alliance tab of the 'chat window'."/>
            </widget>
            <widget name="alicoptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="245" width="510" height="40" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="'Alliance Chat' - ability to send messages in the alliance tab of the 'chat window'."/>
            </widget>
            <widget name="gbanoptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="245" width="510" height="40" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="'Guild Bank' - ability to use the guild bank at any suitable NPC. To activate it, select the NPC and type '/bank guild'."/>
            </widget>      


            <!-- Permission labels -->
            <widget name="" factory="pawsButton" tooltip="View Chat">
                <frame x="122" y="5" width="30" height="30" />
                <bgimage resource="GuildReadIcon" />
                <eventMouseDown>Ret=chatoptdes:Show();Ret=vchatoptdes:Hide();Ret=invoptdes:Hide();Ret=removoptdes:Hide();Ret=promooptdes:Hide();Ret=prioptdes:Hide();Ret=poioptdes:Hide();Ret=editoptdes:Hide();Ret=pubnoptdes:Hide();Ret=prinoptdes:Hide();Ret=valicoptdes:Hide();Ret=alicoptdes:Hide();Ret=gbanoptdes:Hide();Ret=desdefault:Hide()</eventMouseDown>
            </widget>

            <widget name="" factory="pawsButton" tooltip="Chat">
                <frame x="152" y="5" width="30" height="30" />
                <bgimage resource="GuildWriteIcon" />
                <eventMouseDown>Ret=vchatoptdes:Show();Ret=chatoptdes:Hide();Ret=invoptdes:Hide();Ret=removoptdes:Hide();Ret=promooptdes:Hide();Ret=prioptdes:Hide();Ret=poioptdes:Hide();Ret=editoptdes:Hide();Ret=pubnoptdes:Hide();Ret=prinoptdes:Hide();Ret=valicoptdes:Hide();Ret=alicoptdes:Hide();Ret=gbanoptdes:Hide();Ret=desdefault:Hide()</eventMouseDown>
            </widget>

