<widget_description>
    <widget name="GuildWindow" factory="pawsGuildWindow" savepositions="yes" visible="no" movable="yes" resizable="no" configurable="yes" style="New Standard GUI">
        <frame x="100" y="200" width="520" height="380" border="yes"/>
        <title resource="Scaling Title Bar" text="Guild" align="left" close_button="yes"/>

        <!--
            <childborders>
            <colour r="0" g="0" b="0" />
            <colour r="0" g="0" b="255" />
            <colour r="32" g="32" b="32" />
            <colour r="148" g="138" b="255" />
            <colour r="64" g="64" b="64" />
            </childborders>
        -->



        <widget name="Settings_Tab" factory="pawsButton" id="203" toggle="yes" shadows="yes" style="Standard Tab" >
            <frame x="0" y="5" width="80" height="25" />
            <attachpoints>
                <attach point="PROPORTIONAL_RIGHT" />
                <attach point="PROPORTIONAL_LEFT" />
            </attachpoints>
            <label text="Settings" />
        </widget>

        <widget name="Members_Tab" factory="pawsButton" id="201" toggle="yes" shadows="yes" style="Standard Tab" >
            <frame x="85" y="5" width="80" height="25" />
            <attachpoints>
                <attach point="PROPORTIONAL_RIGHT" />
                <attach point="PROPORTIONAL_LEFT" />
            </attachpoints>
            <label text="Members" />
        </widget>

        <widget name="Permissions_Tab" factory="pawsButton" id="200" toggle="yes" shadows="yes" style="Standard Tab" >
            <frame x="170" y="5" width="80" height="25" />
            <attachpoints>
                <attach point="PROPORTIONAL_RIGHT" />
                <attach point="PROPORTIONAL_LEFT" />
            </attachpoints>
            <label text="Permissions" />
        </widget>

        <widget name="Alliances_Tab" factory="pawsButton" id="202" toggle="yes" shadows="yes" style="Standard Tab" >
            <frame x="255" y="5" width="80" height="25" />
            <attachpoints>
                <attach point="PROPORTIONAL_RIGHT" />
                <attach point="PROPORTIONAL_LEFT" />
            </attachpoints>
            <label text="Alliance" />
        </widget>


        <!-- PERMISSIONS PANEL -->

        <widget name="Permissions_Panel" factory="pawsWidget">
            <frame x="0" y="30" width="555" height="350" border="yes"/>

            <!-- Information widgets -->

            <widget name="" factory="pawsTextBox">
                <frame x="5" y="210" width="420" height="30" />
                <text string="Select permissions label for description:"/>
            </widget>


            <widget name="desdefault" factory="pawsMultiLineTextBox" style="Standard Font" visible="yes" border="yes">
                <frame x="5" y="235" width="420" height="50" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string=""/>
            </widget>
            <widget name="chatoptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="235" width="420" height="50" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="When the 'View Chat' option is selected guild members of the level for which it is selected will be able to read all text that is said in the guild chat tab in the 'chat window'"/>
            </widget>
            <widget name="vchatoptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="235" width="420" height="50" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="When the 'Chat' option is selected guild members of the level for which it is selected will be able to chat to other guild members in the guild chat tab in the 'chat window'."/>
            </widget>
            <widget name="invoptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="235" width="420" height="50" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="When the 'Invite' option is selected guild members of the level for which it is selected will be able to invite any other player they wish into the guild."/>
            </widget>
            <widget name="removoptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="235" width="420" height="50" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="When the 'Remove' option is selected guild members of the level for which it is selected will be able to remove any  player they wish from the guild."/>
            </widget>
            <widget name="promooptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="235" width="420" height="50" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="When the 'Promote' option is selected guild members of the level for which it is selected will be able to promote other guild members to levels below that which the player currently has."/>
            </widget>
            <widget name="prioptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="235" width="420" height="50" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="When the 'Privileges' option is selected guild members of the level for which it is selected will be able to adjust which permissions are active for each guild level."/>
            </widget>
            <widget name="poioptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="235" width="420" height="50" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="When the 'Point' option is selected guild members of the level for which it is selected will be able to adjust which permissions are active for each guild level."/>
            </widget>
            <widget name="editoptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="235" width="420" height="50" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="When the 'Edit' option is selected guild members of the level for which it is selected will be able to edit the guild settings on the on the 'settings' tab of the guild window."/>
            </widget>
            <widget name="pubnoptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="235" width="420" height="50" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="When the 'Public Note' option is selected guild members of the level for which it is selected will be able to see and edit the public notes for guild members. You can view or write them on the 'Members' page of the guild window."/>
            </widget>
            <widget name="prinoptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="235" width="420" height="50" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="When the 'Private Note' option is selected guild members of the level for which it is selected will be able to see and edit private notes on guild members. You can view or write them on the 'Members' page of the guild window."/>
            </widget>
            <widget name="valicoptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="235" width="420" height="50" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="When the 'View Alliance Chat' option is selected guild members of the level for which it is selected will be able to read all the talking that occurs in the Alliance tab of the 'Chat window'."/>
            </widget>
            <widget name="alicoptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="235" width="420" height="50" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="When the 'Alliance Chat' option is selected guild members of the level for which it is selected will be able to talk with all others in the alliance in the alliance chat tab of the 'Chat window'."/>
            </widget>
            <widget name="alicoptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="235" width="420" height="50" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="When the 'Alliance Chat' option is selected guild members of the level for which it is selected will be able to talk with all others in the alliance in the alliance chat tab of the 'Chat window'."/>
            </widget>
            <widget name="gbanoptdes" factory="pawsMultiLineTextBox" style="Standard Font" visible="no">
                <frame x="5" y="235" width="420" height="50" />
                <bgimage resource="Scaling Widget Background" alpha="128"/>
                <text string="When the 'Guild Bank' option is selected guild members of the level for which it is selected will be able to use the guild bank at any suitable NPC. To activate it you must select the NPC and use the following command '/bank guild' which will open a new window where coins can be added or removed."/>
            </widget>      


            <!-- Permission labels -->
            <widget name="" factory="pawsTextBox">
                <frame x="131" y="0" width="25" height="30" />
                <text string="A"/>
                <eventMouseDown>Ret=chatoptdes:Show();Ret=vchatoptdes:Hide();Ret=invoptdes:Hide();Ret=removoptdes:Hide();Ret=promooptdes:Hide();Ret=prioptdes:Hide();Ret=poioptdes:Hide();Ret=editoptdes:Hide();Ret=pubnoptdes:Hide();Ret=prinoptdes:Hide();Ret=valicoptdes:Hide();Ret=alicoptdes:Hide();Ret=gbanoptdes:Hide();Ret=desdefault:Hide()</eventMouseDown>
            </widget>

