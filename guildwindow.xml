                <eventMouseDown>Ret=prinoptdes:Show();Ret=chatoptdes:Hide();Ret=vchatoptdes:Hide();Ret=invoptdes:Hide();Ret=removoptdes:Hide();Ret=promooptdes:Hide();Ret=prioptdes:Hide();Ret=poioptdes:Hide();Ret=editoptdes:Hide();Ret=pubnoptdes:Hide();Ret=valicoptdes:Hide();Ret=alicoptdes:Hide();Ret=gbanoptdes:Hide();Ret=desdefault:Hide()</eventMouseDown>
            </widget>

            <widget name="" factory="pawsButton" tooltip="View Alliance Chat">
                <frame x="422" y="5" width="30" height="30" />
                <bgimage resource="GuildAllianceReadIcon" />
                <eventMouseDown>Ret=valicoptdes:Show();Ret=chatoptdes:Hide();Ret=vchatoptdes:Hide();Ret=invoptdes:Hide();Ret=removoptdes:Hide();Ret=promooptdes:Hide();Ret=prioptdes:Hide();Ret=poioptdes:Hide();Ret=editoptdes:Hide();Ret=pubnoptdes:Hide();Ret=prinoptdes:Hide();Ret=alicoptdes:Hide();Ret=gbanoptdes:Hide();Ret=desdefault:Hide()</eventMouseDown>
            </widget>

